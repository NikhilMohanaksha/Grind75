public class Solution {

    public int[] TwoSum(int[] nums, int target) {
    
        Dictionary<int,int> dict = new();
        
        for(int i=0; i<nums.Length; i++){
            int num = nums[i];
            int diff = target - num;
            
            if ( dict.TryGetValue(diff, out int index)){
                return new[]{
                    index, i
                };
            }
            dict[num] = i;
        }
        
       return Array.Empty < int > (); 
    }
}
